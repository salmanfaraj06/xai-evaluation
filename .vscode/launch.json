{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug HEXEval UI",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "${workspaceFolder}/hexeval/ui/app.py",
                "--server.port=8501",
                "--server.headless=false"
            ],
            "jinja": true,
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}